{
  "version": 2,
  "functions": {
    "api/api.php": {
      "runtime": "now-php@1.0.0"
    }
  },
  "routes": [
    {
      "src": "/api/api.php",
      "dest": "/api/api.php"
    },
    {
      "src": "/api/(?!api\\.php$).*",
      "status": 404
    },
    {
      "src": "/api.php",
      "status": 404
    }
  ]
}